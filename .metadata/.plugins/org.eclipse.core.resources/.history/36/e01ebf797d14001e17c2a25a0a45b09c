package festival;

import java.util.Scanner;

public class Ex13우측_별삼각형 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

	
		for (int i = 1; i <= 5; i++) {
			for (int j = 1; j <= 5; j++) {
				System.out.print(" ");
			}
			System.out.print("*");
			System.out.println();
		}
		sc.close();

	}

}
