package festival;

public class Ex08_AtoZ {

	public static void main(String[] args) {
		// A-Z까지 출력

        for(int i = 65; i <=90 ; i++) {
        	int ascii = i;
        	
        System.out.println(ascii); //아스키코드 10진수 출력
        System.out.println(Integer.toHexString(ascii)); // 아스키코드 16진수 출력  
        System.out.println((char)ascii);
		}

	}
}
